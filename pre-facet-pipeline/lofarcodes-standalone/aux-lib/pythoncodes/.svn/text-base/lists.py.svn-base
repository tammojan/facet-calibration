import random

def makelist(start, stop, inc):
    """
    Given start stop and increment this will return a list between those
    """
    value = start
    result = []
    while value <= stop:
        result.append(value)
        value = value + inc
    return result

def randomelement(list):
    """
    Given a list this will return a random index in the list
    """        
    number = random.random()
    element = int(len(list)*number)

    return element
